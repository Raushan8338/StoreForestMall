<resources>

	<!-- Base application theme. -->
	<style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
		<!-- Customize your theme here. -->
		<item name="colorPrimary">@color/colorPrimary</item>
		<item name="colorPrimaryDark">@color/colorPrimaryDark</item>
		<item name="colorAccent">@color/colorAccent</item>
	</style>

	<style name="AppThemes" parent="Theme.AppCompat.Light.DarkActionBar">
		<!-- Customize your theme here. -->
		<item name="colorPrimary">@color/mobile_color</item>
		<item name="colorPrimaryDark">@color/mobile_color</item>
		<item name="colorAccent">@color/mobile_color</item>

	</style>

	<style name="AppTheme.NoActionBar">
		<item name="windowActionBar">false</item>
		<item name="windowNoTitle">true</item>
	</style>
	<style name="textsize" parent="@style/Base.TextAppearance.AppCompat.Title">
		<item name="android:textSize">14dp</item>
		<item name="android:textColor">@color/green</item>
	</style>

	<style name="AppTheme.NoActionBarSuccess">
		<item name="windowActionBar">false</item>
		<item name="windowNoTitle">true</item>
		<item name="colorPrimaryDark">@color/gren</item>
	</style>
	<style name="AppTheme.NoActionBarProduct">
		<item name="windowActionBar">false</item>
		<item name="windowNoTitle">true</item>
		<item name="colorPrimaryDark">@color/colorPrimary</item>
	</style>
	<!--<style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />
    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />-->

</resources>
